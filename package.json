{
  "name": "BaileysHelper",
  "version": "1.0.6",
  "description": "This is a plugin to make baileys more interactive",
  "homepage": "https://github.com/terastudio-org/BaileysHelper",
  "keywords": [
    "Baileys",
    "WhatsApp"
  ],
  "license": "ISC",
  "author": "terastudio-org",
  "type": "commonjs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terastudio-org/BaileysHelper.git"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "tsc",
    "build:types": "tsc --project tsconfig.declarations.json",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "dev": "npm run build:watch"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "bugs": {
    "url": "https://github.com/terastudio-org/BaileysHelper/issues"
  }
}
